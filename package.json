{
  "name": "energy_prices_comparison",
  "version": "2.1.0",
  "description": "Smart German electricity price scraper with city classification, multiple extraction strategies, and complete API integration",
  "main": "scrapers/smart-single-scraper.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "// === SMART SCRAPER (RECOMMENDED) ===": "",
    "scrape:smart": "node scrapers/smart-single-scraper.js",
    "scrape:smart-tor": "node scrapers/rotating-proxy-scraper.js",
    "scrape": "npm run scrape:smart-tor",
    "scrape:proxy": "node scrapers/proxy-enhanced-scraper.js",
    "scrape:rotating": "node scrapers/rotating-proxy-scraper.js",
    "// === ALTERNATIVE SCRAPERS ===": "",
    "scrape:1000-simple": "node run-1000-simple.js",
    "scrape:1000": "node run-1000-cities.js",
    "scrape:1000-new": "node run-1000-new-cities.js",
    "scrape:legacy": "node scrapers/stromauskunft_scraper_batched.js",
    "// === TESTING ===": "",
    "test:batch": "cd scrapers/examples && node basic-usage.js",
    "test:umlauts": "cd scrapers/examples && node test-umlauts.js",
    "test:500": "cd scrapers/examples && node test-500-real.js",
    "// === DATABASE ===": "",
    "db:examples": "node database/examples.js",
    "db:test": "node -e \"const SupabaseClient = require('./database/supabase-client'); const db = new SupabaseClient(); db.testConnection();\"",
    "// === API ===": "",
    "api:start": "node api/server.js",
    "api:dev": "NODE_ENV=development node api/server.js",
    "api:prod": "NODE_ENV=production node api/server.js",
    "api:test": "node api/test-api.js",
    "// === DATA QUALITY CLEANUP ===": "",
    "cleanup:analyze": "node scripts/run-data-cleanup.js --analyze-only",
    "cleanup:test": "node scripts/run-data-cleanup.js --max-entries 10",
    "cleanup:run": "node scripts/run-data-cleanup.js",
    "cleanup:month": "node scripts/run-data-cleanup.js --month",
    "analyze:issues": "node scripts/analyze-price-issues.js",
    "analyze:cities": "node scripts/analyze-price-issues.js --big-cities",
    "analyze:url": "node scripts/analyze-price-issues.js --url",
    "fix:oeko": "node scripts/fix-missing-oeko.js",
    "fix:oeko-test": "node scripts/fix-missing-oeko.js --max-entries 5",
    "fix:high-prices": "node scripts/fix-high-prices.js",
    "fix:high-prices-test": "node scripts/fix-high-prices.js --max-entries 5",
    "// === DEPLOYMENT ===": "",
    "deploy:vercel": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "keywords": [
    "electricity",
    "energy",
    "prices",
    "germany",
    "scraper",
    "supabase",
    "smart-classification",
    "extraction-strategies",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "https-proxy-agent": "^7.0.6",
    "morgan": "^1.10.0",
    "socks-proxy-agent": "^8.0.5"
  },
  "devDependencies": {
    "vercel": "^44.5.3"
  }
}
